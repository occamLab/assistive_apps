# Uncomment the next line to define a global platform for your project
platform :ios, '12.0'

use_frameworks!

def all_pods
  # Pods for Clew
  pod 'InAppSettingsKit'
  pod 'VectorMath', '~> 0.3'
  pod 'Firebase/Core'
  pod 'Firebase/Storage'
  pod 'Firebase/Database'
  pod 'SRCountdownTimer'
end

target 'Clew' do
    all_pods
end

target 'Clew Dev' do
    all_pods
end
